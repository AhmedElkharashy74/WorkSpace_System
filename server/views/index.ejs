      <script>
        document.getElementById('guestSearchInput').addEventListener('input', function() {
          const query = this.value.trim();
          if (query.length > 0) {
            fetch(`/searchGuests?q=${encodeURIComponent(query)}`)
              .then(response => response.json())
              .then(data => {
                const suggestionsList = document.getElementById('suggestionsList');
                suggestionsList.innerHTML = '';
                data.forEach(guest => {
                  const listItem = document.createElement('li');
                  listItem.textContent = guest.name;
                  listItem.classList.add('p-2', 'cursor-pointer', 'hover:bg-gray-200');
                  listItem.addEventListener('click', function() {
                    document.getElementById('guestSearchInput').value = guest.name;
                    suggestionsList.classList.add('hidden');
      
                    // Create the hidden input field
                    const hiddenInput = document.createElement('input');
                    hiddenInput.type = 'hidden';
                    hiddenInput.value = guest.id;
                    hiddenInput.setAttribute('name', 'userId');
      
                    // Append the hidden input to the form
                    const form = document.getElementById('guestForm');
                    const existingHiddenInput = form.querySelector('input[name="userId"]');
                    if (existingHiddenInput) {
                      existingHiddenInput.value = guest.id;
                    } else {
                      form.appendChild(hiddenInput);
                    }
                  });
                  suggestionsList.appendChild(listItem);
                });
                suggestionsList.classList.remove('hidden');
              })
              .catch(error => {
                console.error('Error fetching suggestions:', error);
              });
          } else {
            document.getElementById('suggestionsList').classList.add('hidden');
          }
        });
      
