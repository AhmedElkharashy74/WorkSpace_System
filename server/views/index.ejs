              href="#users"
              class="p-3 transition-shadow border rounded-lg shadow-sm hover:shadow-lg"
            >
              <div class="flex items-start">
                <div class="flex flex-col flex-shrink-0 space-y-2">
                  <span class="text-gray-400">Total Guests</span>
                  <span class="text-lg font-semibold"><%= active.length%></span>
                </div>
                <div class="relative min-w-0 ml-auto h-14">
                  <!-- <canvas id="usersChart"></canvas> -->
                </div>
              </div>
              <div>
                <span class="inline-block px-2 text-sm text-white bg-green-300 rounded"><%= Math.round(active.length*100/freeSpace) %> %</span>
                <span>of <%= totalSpace %></span>
              </div>
            </a>
            <!-- Sessions chart card --><a
              href="#freerooms"
